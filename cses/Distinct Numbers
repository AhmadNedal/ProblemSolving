#include <bits/stdc++.h>
#define ll long long
using namespace std;
 
 
void solve() {
 
 
    ll n,num; cin>>n;
    set <ll>s;
    while (n--) {
        cin>>num;
        s.insert(num) ;
    }
 
    cout << s.size() <<endl;
 
}
 
int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);
 
    int t=1;//cin >>t ;
 
    while (t--)solve();
 
    return 0;
}
